    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Additive techniques of manufacturing</ns2:hasName>
    <ns2:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns2:hasLanguage>
