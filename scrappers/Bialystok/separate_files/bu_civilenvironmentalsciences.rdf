  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFCEE00222W">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Environmental administration and management</ns1:hasName>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns1:hasLanguage>
