  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#FCS00061">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Information Technologies</ns1:hasName>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns1:hasLanguage>
    <ns1:hasProgrammeType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Engineer's degree full-time programme</ns1:hasProgrammeType>
