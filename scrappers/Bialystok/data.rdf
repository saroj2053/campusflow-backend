    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#string">56</ns1:hasWorkLoad>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IS-FME -00140W</ns1:hasModuleNumber>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Introduction to digital signal processing. Discrete -time Fourier transform. Short time Fourier transform. Sampling. Properties of digital filters. Finite Impulse Response filters. I nfinite Impulse Response filters. Adaptive filters. Wavelet transform.</ns1:hasContent>
