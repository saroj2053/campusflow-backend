  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFME00250S">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fundamen tals of Mechanical Engineering</ns1:hasName>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns1:hasLanguage>
    <ns1:hasProgrammeType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bachelor's degree</ns1:hasProgrammeType>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#string">136</ns1:hasWorkLoad>
