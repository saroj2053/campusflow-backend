  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFM00016S">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Quality management and control</ns1:hasName>
