    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Web Applications Development in Java</ns1:hasName>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns1:hasLanguage>
