  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFCEE00265WS">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HVAC units and installations in reliability theory</ns1:hasName>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns1:hasLanguage>
