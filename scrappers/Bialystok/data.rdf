  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFCEE00238W">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Public space arrangement</ns1:hasName>
