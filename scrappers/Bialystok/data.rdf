    <ns1:hasProgrammeType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bachelor/Master</ns1:hasProgrammeType>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#string">90</ns1:hasWorkLoad>
