  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFM00077W">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Business Forecasting</ns1:hasName>
