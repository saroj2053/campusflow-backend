    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">INF1LDI</ns1:hasModuleNumber>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Algebra of sets. Propositional calculus. Logic of predicates. Axioms and inference rules. Satisfiability. Tautologies. The notion of formal proof. Relations. Equivalence relations. Ordered sets. Functions.</ns1:hasContent>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3</ns1:hasCreditPoints>
