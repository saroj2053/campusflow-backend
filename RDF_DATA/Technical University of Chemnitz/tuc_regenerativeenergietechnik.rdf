  <rdf:Description rdf:about="http://tuc.web.engineering/module#9aed2930-e06d-11ee-8d47-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">242033-007(Version01)</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Statistik, Zuverlässigkeit und Isolationskoordination</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Empirische statistische und theoretische Verteilungsfunktionen  Nachweis der Unabhängigkeit von Messreihen durch statistische Testverfahren, Planung von Versuchen  Vergrößerungsgesetz  Anpassung des Isoliervermögens an zu erwartende Beanspruchungen  Ermittlung der Punktverfügbarkeit in elektrischen Netzen</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns2:hasWorkLoad>
