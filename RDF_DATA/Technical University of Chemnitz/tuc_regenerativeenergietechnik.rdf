    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">242032-003(Version01)</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bauelemente der Leistungselektronik / Power Semiconductor Devices</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Besonderheiten leistungselektronischer Bauelemente 2. Halbleiterphysikalische Grundlagen 2.1 Eigenschaften der Halbleiter, physikalische Grundlagen 2.2 pn-Übergänge 2.3 Einführung in die Herstellungstechnologie 3. Halbleiterbauelemente 3.1 pin Dioden 3.2 Schottky-Dioden 3.3 Bipolare Transistoren 3.4 Thyristoren 3.5 MOS-Transistoren 3.6 IGBTs 4. Einführung in die Aufbau- und Verbindungstechnik</ns2:hasContent>
