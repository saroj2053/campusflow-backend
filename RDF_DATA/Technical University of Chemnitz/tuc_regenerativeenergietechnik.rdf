  <rdf:Description rdf:about="http://tuc.web.engineering/module#9aede48f-e06d-11ee-b1fa-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">264032-207(Version01)</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Recht und Technik (Technikrecht)</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Technikrecht/Technologierecht/Recht neuer Technologien  Aufzeigen der Schnittstellen von Recht und Technik  Produktverantwortung/-haftung (zivil- und strafrechtliche Grundlagen – auch rechtsvergleichend)  Normung, Zertifizierung und Akkreditierung  Europäische und nationale Marktüberwachung  Aktuelle Themen mit technikrechtlichem Bezug (je nach Teilnehmerkreis), z. B. Cloud-Computing, E-Commerce, Elektromobilität, Industrie 4.0, Künstliche Intelligenz</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns2:hasWorkLoad>
