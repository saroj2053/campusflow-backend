    <ns2:hasModules rdf:resource="http://tuc.web.engineering/module#a56e16f3-d9b5-11ee-a634-cc153190a929"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#4b428d31-da25-11ee-8f41-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">543110</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Datensicherheit und Kryptographie II</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Es werden aktuelle kryptographische Verfahren aus folgenden Themengebieten betrachtet:  Visuelle Kryptographie  Secret Sharing Schemata  Kryptographische Protokolle</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns2:hasWorkLoad>
