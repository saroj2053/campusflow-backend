    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M27</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Statistik in Data Science</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Explorative Datenanalyse (erkundende Statistik)  Deskriptive Statistik  Large sample theory  Mathematische Statistik  Asymptotische Statistik  Extremwertstatistik  Large deviation theory</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">180</ns2:hasWorkLoad>
    <ns1:hasUniversity rdf:resource="http://across/university#TUC"/>
    <ns3:hasCourse rdf:resource="http://tuc/course#TUCDataScience"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#8908d842-da24-11ee-a68d-cc153190a929">
