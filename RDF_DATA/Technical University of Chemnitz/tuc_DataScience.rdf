  <rdf:Description rdf:about="http://tuc.web.engineering/module#8908ff61-da24-11ee-8e1e-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M29</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mathematische Methoden der Quantifizierung von Unsicherheit</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Beschreibungen von Unsicherheit  Numerik zufälliger Differentialgleichungen  Bayessche Inferenz für inverse Probleme</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</ns2:hasCreditPoints>
