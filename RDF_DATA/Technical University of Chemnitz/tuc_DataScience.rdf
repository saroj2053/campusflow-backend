    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">E21</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Regelungstechnik 2B</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Lineare Mehrgrößensysteme und -regelungen  Modellreduktion  Beobachterentwurf  erweiterte Konzepte der Mehrgrößenregelung</ns2:hasContent>
