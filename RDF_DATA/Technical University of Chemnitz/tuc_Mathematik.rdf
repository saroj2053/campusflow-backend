  <rdf:Description rdf:about="http://tuc.web.engineering/module#495259cf-d9b5-11ee-9e17-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M-Ma-I13</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wahrscheinlichkeitsrechnung und Algorithmik</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Es wird gezeigt, wie die Konzepte der Wahrscheinlichkeitsrechnung in der diskreten Algorithmik auftreten. Dazu werden behandelt: Randomisierte Algorithmen und zufällige Eingaben</ns2:hasContent>
