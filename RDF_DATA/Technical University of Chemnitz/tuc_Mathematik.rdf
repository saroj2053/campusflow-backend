  <rdf:Description rdf:about="http://tuc.web.engineering/module#4953fbc4-d9b5-11ee-8754-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">241031-102(Version01)</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Regelungstechnik 1</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Systemanalyse im Zeitbereich  Reglerentwurf im Zeitbereich  Systemanalyse im Frequenzbereich  Analyse von Regelkreisen, Anforderungen an Regelkreise  Reglerentwurf im Frequenzbereich</ns2:hasContent>
