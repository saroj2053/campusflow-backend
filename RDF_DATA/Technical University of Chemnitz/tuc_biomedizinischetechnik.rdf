  <rdf:Description rdf:about="http://tuc.web.engineering/module#98e06d5b-e070-11ee-b4f4-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3.2.10(565130)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verlässliche Systeme</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Begriffe und Metriken der Verlässlichkeit  Störverhalten  Entwurfsmuster für Fehlertoleranz  Analyse von Verlässlichkeitseigenschaften  Diagnose  Ansätze zur Erhöhung der Verlässlichkeit in Hard- und Softwareware</ns1:hasContent>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns1:hasCreditPoints>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns1:hasWorkLoad>
    <ns2:hasUniversity rdf:resource="http://across/university#TUC"/>
