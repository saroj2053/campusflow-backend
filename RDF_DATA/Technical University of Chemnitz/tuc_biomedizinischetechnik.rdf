    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.8</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Medizinrecht und Ethik</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Medizinproduktegesetz, Medizinprodukte-Betreiberverordnung, Medizin- geräteverordnung  Richtlinien für klinische Studien  Bestimmungen und Verfahren zur Einführung neuer Medizingeräte  Prüfarztbroschüre, Zertifizierung  Ethische Aspekte bei der Entwicklung und dem Einsatz von Medizintechnik sowie der Durchführung medizinischer Studien</ns1:hasContent>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns1:hasCreditPoints>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns1:hasWorkLoad>
