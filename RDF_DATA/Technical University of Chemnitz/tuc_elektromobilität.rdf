    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">232034-001(Version05)</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fahrwerktechnik I</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fahrwiderstände Anforderungen an das Fahrwerk für Straßenfahrzeuge und mobile Arbeitsmaschinen Fahrwerk mit Aufbau und Funktionsweise Rad/Reifen o Achsbauweisen o Lenkung o Bremsen o Federung/Dämpfung o Erprobung (Komponentenerprobung, Fahrversuch)</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns2:hasWorkLoad>
    <ns1:hasUniversity rdf:resource="http://across/university#TUC"/>
    <ns3:hasCourse rdf:resource="http://tuc/course#TUCElektromobilität"/>
