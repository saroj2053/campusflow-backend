    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Human Factors</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> kognitive Ergonomie  Arbeitsplatz- und Arbeitsmittelgestaltung  Produktdesign  Mensch-Maschinesysteme  Automatisierung</ns2:hasContent>
