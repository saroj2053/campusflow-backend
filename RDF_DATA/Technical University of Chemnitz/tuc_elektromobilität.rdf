    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">242031-005(Version01)</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Automatisierte Antriebe</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Modellierung elektromechanischer Systeme  Antriebskomponenten und -systeme  Hard- und Softwarekomponenten der Signalverarbeitung des Antriebssystems  Umrichterspeisung frequenzgesteuerter Antriebe  Pulssteuerverfahren zur Umrichterspeisung  Feldorientierte Regelung von Drehstrommaschinen  Wechselwirkungen von Stellglied und Motor</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">210</ns2:hasWorkLoad>
