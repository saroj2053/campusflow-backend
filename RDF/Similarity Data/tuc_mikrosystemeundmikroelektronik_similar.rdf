    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mikrosysteme für die Medizin</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Einsatzmöglichkeiten von Mikrosystemen (MEMS) in der Medizin und Medizintechnik  Anforderungen / Besonderheiten beim Einsatz von Mikrosystemen im medizinischen Umfeld  Aufbau und Wirkprinzipien medizinischer MEMS  Entwurf / Simulation</ns1:hasContent>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns1:hasCreditPoints>
