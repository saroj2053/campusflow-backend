    <ns3:hasCourse rdf:resource="http://tuc/course#TUCMikrosystemeundMikroelektronik"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#f2fe4a09-e06e-11ee-80eb-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">244033-013(Version01)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gerätetechnik</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Konstruktionsmethodik (Analysieren und Gestalten von Geräten)  Funktionsgruppen der Gerätetechnik (Lager und Führungen, Achsen und Wellen, Gehemme und Gesperre, Anschläge, Bremsen und Dämpfer, Kupplungen, Getriebe und Energiewandler)  Praktika zu Funktionsgruppen der Gerätetechnik</ns1:hasContent>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns1:hasCreditPoints>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns1:hasWorkLoad>
