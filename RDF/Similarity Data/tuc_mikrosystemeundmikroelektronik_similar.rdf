  <rdf:Description rdf:about="http://tuc.web.engineering/module#f2ff06c3-e06e-11ee-a077-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">240100-824(Version01)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Master-Arbeit</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gegenstand des Moduls ist die Erstellung der Masterarbeit zu einer ingenieurwissenschaftlichen Aufgabe, deren schriftliche Darstellung und eine mündliche Prüfung. Das Thema der Masterarbeit soll auf dem Gebiet der Mikrosystemtechnik, Gerätetechnik oder Mikroelektronik liegen. Der Student wird dabei von einem wissenschaftlichen Betreuer der Fakultät unterstützt.</ns1:hasContent>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</ns1:hasCreditPoints>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">900</ns1:hasWorkLoad>
    <ns2:hasUniversity rdf:resource="http://across/university#TUC"/>
