  <rdf:Description rdf:about="http://tuc.web.engineering/module#54a538c0-e06f-11ee-a4f2-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">244038-602(Version01)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sensor Signal Evaluation</ns1:hasName>
